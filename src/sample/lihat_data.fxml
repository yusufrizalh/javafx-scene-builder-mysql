<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LihatData">
   <children>
      <VBox prefHeight="300.0" prefWidth="500.0">
         <children>
            <TableView fx:id="tabel_peserta" prefHeight="300.0" prefWidth="506.0">
              <columns>
                <TableColumn fx:id="col_id" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="col_nama" prefWidth="75.0" text="NAMA" />
                  <TableColumn fx:id="col_email" prefWidth="75.0" text="EMAIL" />
                  <TableColumn fx:id="col_password" prefWidth="75.0" text="PASSWORD" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
